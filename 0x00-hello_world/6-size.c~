#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
    char char_var;
    int int_var;
    long int l_int_var;
    long long int ll_int_var;
    float f_int;

    printf("Size of a cahr:", sizeof(char_var), "byte(s)");
    printf("Size of a cahr:", sizeof(int_var), "byte(s)");
    printf("Size of a cahr:", sizeof(l_int_var), "byte(s)");
    printf("Size of a cahr:", sizeof(ll_int_var), "byte(s)");
    printf("Size of a cahr:", sizeof(f_int), "byte(s)");
}
